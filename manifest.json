{
    "manifest_version": 2,
    "name": "youtube ass subtitle",
    "version": "0.1.3",
    "description": "apply ass subtitle at youtube",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "image/16.png",
            "32": "image/32.png",
            "64": "image/64.png"
        },
        "page_url": "https://*.youtube.com/*"
    },
    "background": {
        "persistent": false,
        "scripts": [ "src/worker_proxy/worker_proxy.js", "src/background.js"]
    },
    "content_scripts": [
        {
            "matches": ["https://*.youtube.com/*"],
            "js": [ "src/worker_proxy/worker_proxy.js", "src/subtitles_octopus/subtitles-octopus.js", "src/loader.js", "src/storage.js"]
        }
    ],
    "content_security_policy": "script-src 'self' 'wasm-eval'; object-src 'self'",
    "web_accessible_resources": ["worker_proxy.html", "options.html"],
    "permissions": [ "https://*.youtube.com/*", "storage", "unlimitedStorage" ],
    "icons": {
        "16": "image/16.png",
        "32": "image/32.png",
        "64": "image/64.png"
    },
    "options_ui": {
      "page": "options/options.html",
      "open_in_tab": true
    }
  }